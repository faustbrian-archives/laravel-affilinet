{
    "name": "artisanry/affilinet",
    "description": "An Affilinet bridge for Laravel",
    "keywords": [
        "laravel",
        "framework",
        "Laravel-Affilinet",
        "Laravel Affilinet"
    ],
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "hello@basecode.sh",
            "homepage": "https://basecode.sh"
        }
    ],
    "require": {
        "php": "^7.2",
        "faustbrian/affilinet-php": "dev-master",
        "graham-campbell/manager": "^3.0",
        "illuminate/support": "^5.8"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\Affilinet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\Affilinet\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\Affilinet\\AffilinetServiceProvider"
            ],
            "aliases": {
                "Affilinet": "Artisanry\\Affilinet\\Facades\\Affilinet"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
