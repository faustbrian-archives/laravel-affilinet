{
    "name": "faustbrian/laravel-affilinet",
    "description": "An Affilinet bridge for Laravel",
    "keywords": ["laravel", "framework", "Laravel-Affilinet", "Laravel Affilinet", "Brian Faust", "faustbrian"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.me",
        "homepage": "https://brianfaust.me"
    }],
    "require": {
        "php": "^7.1",
        "faustbrian/affilinet-php": "dev-master",
        "graham-campbell/manager": "^3.0",
        "illuminate/support": "5.5.* || 5.6.* || 5.7.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\Affilinet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\Affilinet\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "BrianFaust\\Affilinet\\AffilinetServiceProvider"
            ],
            "aliases": {
                "Affilinet": "BrianFaust\\Affilinet\\Facades\\Affilinet"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
